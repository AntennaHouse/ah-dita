      <moduleMetadata>
         <moduleType>topicshell</moduleType>
         <moduleShortName>Glossary Entry</moduleShortName>
         <shellPublicIds>
            <dtdShell>-//ANTENNA HOUSE//DTD DITA<var presep=" " name="ditaver"/> Glossary Entry//EN</dtdShell>
            <rncShell>urn:x-antennahouse:dita:rnc:glossentry.rnc<var presep=":" name="ditaver"/>
            </rncShell>
            <rngShell>urn:x-antennahouse:dita:rng:glossentry.rng<var presep=":" name="ditaver"/>
            </rngShell>
            <xsdShell>urn:x-antennahouse:dita:xsd:glossentry.xsd<var presep=":" name="ditaver"/>
            </xsdShell>
         </shellPublicIds>
      </moduleMetadata>
  </moduleDesc>
  <div>
      <a:documentation>ROOT ELEMENT DECLARATION</a:documentation>
      <start>
         <ref name="glossentry.element"/>
      </start>
  </div>
  <div>
      <a:documentation>DOMAINS ATTRIBUTE</a:documentation>
      <define name="domains-att" combine="interleave">
         <optional>
            <attribute name="domains"
                       a:defaultValue="(topic abbrev-d)
                         (topic concept glossentry)
                         (topic concept)
                         (topic equation-d)
                         (topic hazard-d)
                         (topic hi-d)
                         (topic indexing-d)
                         (topic markup-d xml-d)
                         (topic markup-d)
                         (topic mathml-d)
                         (topic pr-d)
                         (topic relmgmt-d)
                         (topic svg-d)
                         (topic sw-d)
                         (topic ui-d)
                         (topic ut-d)
                         (topic ch-d)
                         (topic floatfig-d)
                         a(props deliveryTarget)
                         a(base fo:prop)
                         a(base css:prop)
                         "/>
         </optional>
      </define>
  </div>
  <div>
      <a:documentation>MODULE INCLUSIONS</a:documentation>
      <include href="urn:oasis:names:tc:dita:rng:topicMod.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:conceptMod.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:glossentryMod.rng">
        <define name="glossentry-info-types">
          <empty/>
        </define>
      </include>
      <include href="urn:oasis:names:tc:dita:rng:deliveryTargetAttDomain.rng" dita:since="1.3"/>
      <include href="urn:oasis:names:tc:dita:rng:equationDomain.rng" dita:since="1.3"/>
      <include href="urn:oasis:names:tc:dita:rng:hazardDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:highlightDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:indexingDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:abbreviateDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:markupDomain.rng" dita:since="1.3"/>
      <include href="urn:oasis:names:tc:dita:rng:mathmlDomain.rng" dita:since="1.3"/>
      <include href="urn:oasis:names:tc:dita:rng:programmingDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:releaseManagementDomain.rng" dita:since="1.3"/>
      <include href="urn:oasis:names:tc:dita:rng:softwareDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:svgDomain.rng" dita:since="1.3"/>
      <include href="urn:oasis:names:tc:dita:rng:uiDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:utilitiesDomain.rng"/>
      <include href="urn:oasis:names:tc:dita:rng:xmlDomain.rng" dita:since="1.3"/>
      <include href="urn:x-antennahouse:dita:rng:characterDomain.rng"/>
      <include href="urn:x-antennahouse:dita:rng:floatfigDomain.rng"/>
      <include href="urn:x-antennahouse:dita:rng:xslfoAttDomain.rng"/>
      <include href="urn:x-antennahouse:dita:rng:cssAttDomain.rng"/>
  </div>
  <div>
      <a:documentation>ID-DEFINING-ELEMENT OVERRIDES</a:documentation>
      <define name="any">
         <zeroOrMore>
            <choice>
               <ref name="idElements"/>
               <element>
                  <anyName>
                     <except>
                        <name>topic</name>
                        <name>concept</name>
                        <name>glossentry</name>
                        <nsName ns="http://www.w3.org/2000/svg"/>
                        <nsName ns="http://www.w3.org/1998/Math/MathML"/>
                     </except>
                  </anyName>
                  <zeroOrMore>
                     <attribute>
                        <anyName/>
                     </attribute>
                  </zeroOrMore>
                  <ref name="any"/>
               </element>
               <text/>
            </choice>
         </zeroOrMore>
      </define>
  </div>
</grammar>
